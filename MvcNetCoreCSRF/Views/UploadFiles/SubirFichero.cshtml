<h1>Upload Files Net Core</h1>


<form method="post" enctype="multipart/form-data">
    <label>Seleccione un fichero</label>
    <input type="file" name="fichero" class="form-control" />
    <button class="btn btn-outline-info">Subir fichero</button>
</form>

<h2 style="color:darkkhaki">@ViewData["MENSAJE"]</h2>

<img src='@ViewData["URL"]' style="width: 150px; height: 150px"/>